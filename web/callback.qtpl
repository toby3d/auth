{% import "source.toby3d.me/website/indieauth/internal/domain" %}

{% code type CallbackPage struct {
  BaseOf
  Token *domain.Token
} %}

{% collapsespace %}
{% func (p *CallbackPage) Body() %}
  {% if p.Token != nil %}
    <h1>{%s p.Token.Me.String() %}</h1>
    <small>{%s p.Token.AccessToken %}</small>
  {% endif %}
{% endfunc %}
{% endcollapsespace %}
